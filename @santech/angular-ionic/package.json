{
  "name": "@santech/angular-ionic",
  "version": "7.0.0-beta.4",
  "description": "Ionic module for santech studio",
  "main": "dist/bundles/santech-angular-ionic.umd.js",
  "module": "dist/esm5/santech-angular-ionic.js",
  "es2015": "dist/esm2015/santech-angular-ionic.js",
  "types": "dist/index.d.ts",
  "typings": "dist/santech-angular-ionic.d.ts",
  "scripts": {
    "build": "npm run clean && ts-node build.ts && npm run docs && npm run package",
    "clean": "rimraf dist docs coverage .ng-build dist.tgz",
    "docs": "typedoc src/index.ts",
    "lint": "tslint \"**/*.ts\" \"**/*.js\" -e \"node_modules/**\"",
    "package": "npm pack dist/ && mv santech-angular-ionic-*.tgz dist.tgz",
    "test": "jest --coverage",
    "test-watch": "jest --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/santech-org/studio"
  },
  "keywords": [
    "santech",
    "studio",
    "angular",
    "ionic"
  ],
  "license": "MIT",
  "author": "Paul Souche <psouche@santech.fr>",
  "peerDependencies": {
    "@angular/common": "^7.0.0",
    "@angular/core": "^7.0.0",
    "@ionic-native/camera": "^5.0.0-beta.0",
    "@ionic-native/core": "^5.0.0-beta.0",
    "@ionic-native/file": "^5.0.0-beta.0",
    "@ionic-native/keyboard": "^5.0.0-beta.0",
    "@ionic-native/network": "^5.0.0-beta.0",
    "@ionic-native/onesignal": "^5.0.0-beta.0",
    "@ionic-native/splash-screen": "^5.0.0-beta.0",
    "@ionic/angular": "^4.0.0-beta.0",
    "@ionic/core": "^4.0.0-beta.0",
    "@ionic/storage": "^2.0.0",
    "@santech/analytics-core": "^7.0.0-alpha.0",
    "@santech/analytics-integration": "^7.0.0-alpha.0",
    "@santech/angular-analytics": "^7.0.0-alpha.0",
    "@santech/angular-common": "^7.0.0-alpha.0",
    "@santech/angular-cropper": "^7.0.0-alpha.0",
    "@santech/angular-pica": "^7.0.0-alpha.0",
    "@santech/angular-platform": "^7.0.0-alpha.0",
    "@santech/core": "^7.0.0-alpha.0",
    "rxjs": "^6.0.0"
  },
  "devDependencies": {
    "@angular/common": "*",
    "@angular/compiler": "*",
    "@angular/compiler-cli": "*",
    "@angular/core": "*",
    "@angular/forms": "*",
    "@angular/platform-browser": "*",
    "@angular/platform-browser-dynamic": "*",
    "@ionic-native/camera": "^5.0.0-beta.0",
    "@ionic-native/core": "^5.0.0-beta.0",
    "@ionic-native/file": "^5.0.0-beta.0",
    "@ionic-native/keyboard": "^5.0.0-beta.0",
    "@ionic-native/network": "^5.0.0-beta.0",
    "@ionic-native/onesignal": "^5.0.0-beta.0",
    "@ionic-native/splash-screen": "^5.0.0-beta.0",
    "@ionic/angular": "^4.0.0-beta.0",
    "@ionic/core": "^4.0.0-beta.0",
    "@ionic/storage": "*",
    "@santech/angular-analytics": "^7.0.0-beta.4",
    "@santech/angular-common": "^7.0.0-beta.4",
    "@santech/angular-cropper": "^7.0.0-beta.4",
    "@santech/angular-pica": "^7.0.0-beta.4",
    "@santech/angular-platform": "^7.0.0-beta.4",
    "@santech/common": "^7.0.0-beta.4",
    "@santech/core": "^7.0.0-beta.4",
    "@types/jest": "*",
    "@types/node": "*",
    "@types/onesignal-cordova-plugin": "*",
    "cpx": "*",
    "hammerjs": "^2.0.0",
    "jest": "*",
    "jest-localstorage-mock": "*",
    "jest-preset-angular": "^6.0.0-rc.0",
    "ng-packagr": "*",
    "rimraf": "*",
    "rxjs": "*",
    "ts-node": "*",
    "tsickle": "*",
    "tslib": "*",
    "tslint": "*",
    "typedoc": "*",
    "typescript": "*",
    "whatwg-fetch": "*",
    "zone.js": "*"
  },
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.(ts|js|html)$": "<rootDir>../../node_modules/jest-preset-angular/preprocessor.js"
    },
    "moduleNameMapper": {
      "^@ionic/core/dist/ionic/svg": "<rootDir>/package.json",
      "^@ionic/core/loader": "<rootDir>../../node_modules/@ionic/core/dist/esm/es5/ionic.define.js"
    },
    "preset": "jest-preset-angular",
    "roots": [
      "<rootDir>/src/"
    ],
    "setupFiles": [
      "whatwg-fetch",
      "jest-localstorage-mock"
    ],
    "setupTestFrameworkScriptFile": "<rootDir>/setup-jest.ts",
    "transformIgnorePatterns": [
      "node_modules/(?!@ionic|ngx-pica|ionic)"
    ],
    "snapshotSerializers": [
      "<rootDir>../../node_modules/jest-preset-angular/AngularSnapshotSerializer.js",
      "<rootDir>../../node_modules/jest-preset-angular/HTMLCommentSerializer.js"
    ]
  }
}
