{
  "name": "@santech/websocket",
  "version": "7.0.0-beta.0",
  "description": "websocket connector for santech studio",
  "main": "dist/umd/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist/*",
    "docs/*",
    "testing/*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/santech-org/studio"
  },
  "scripts": {
    "build": "npm run clean && npm run transpile && npm run rollup && npm run docs && npm run package",
    "clean": "rimraf docs dist coverage .rpt2_cache dist.tgz",
    "docs": "typedoc src/index.ts",
    "lint": "tslint \"**/*.ts\" \"**/*.js\" -e \"node_modules/**\"",
    "package": "npm pack && mv santech-websocket-*.tgz dist.tgz",
    "rollup": "node rollup.js",
    "start": "tsc -p . -w",
    "test": "jest --coverage",
    "test-watch": "jest --watch",
    "transpile": "tsc -p . --declaration true && tsc -p tsconfig.testing.json --declaration true"
  },
  "keywords": [
    "santech",
    "studio",
    "websocket"
  ],
  "author": "Paul Souche <psouche@santech.fr>",
  "license": "MIT",
  "dependencies": {
    "@santech/common": "^7.0.0-beta.0",
    "@santech/core": "^7.0.0-beta.0",
    "@stomp/stompjs": "*",
    "@types/sockjs-client": "*",
    "sockjs-client": "*"
  },
  "devDependencies": {
    "@types/jest": "*",
    "@types/sinon": "*",
    "jest": "*",
    "rimraf": "*",
    "rollup": "*",
    "rollup-plugin-typescript2": "*",
    "sinon": "*",
    "ts-jest": "*",
    "ts-node": "*",
    "tslib": "*",
    "tslint": "*",
    "typedoc": "*",
    "typescript": "*"
  },
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.ts$": "ts-jest"
    },
    "testRegex": "^.+\\.spec.ts$",
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
